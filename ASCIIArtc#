using System;

namespace ConsoleCreatures
{
  class Program
  {
    static void Main()
    {
      //This program is designed to explore creating ASCII art
      Console.WriteLine(" .-.");
      Console.WriteLine("(o o)");
      Console.WriteLine("| O |");
      Console.WriteLine("|   |");
      Console.WriteLine("'~~~'");
      Console.WriteLine("Spoopy Ghost");    
     }
  }
}
